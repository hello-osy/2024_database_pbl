 class=\"app-wrapper\">\n    <!-- Sidebar 유지 -->\n    <sidebar />\n\n    <!-- 메인 콘텐츠 -->\n    <div class=\"main-content\">\n      <notifications></notifications>\n      <router-view></router-view>\n\n      <!-- Stats Cards Section -->\n      <div class=\"stats-row\">\n        <div\n          class=\"stats-card\"\n          v-for=\"stats in statsCards\"\n          :key=\"stats.title\"\n        >\n          <div class=\"icon-container\" :class=\"`icon-${stats.type}`\">\n            <i :class=\"stats.icon\"></i>\n          </div>\n          <div class=\"stats-info\">\n            <p>{{ stats.title }}</p>\n            <h3>{{ stats.value }}</h3>\n          </div>\n        </div>\n      </div>\n\n      <!-- Custom Section for Yard Layout -->\n      <div class=\"yard-layout\">\n        <div class=\"dock\" v-for=\"(dock, index) in dockStatus\" :key=\"index\">\n          <h3>{{ dock.name }}</h3>\n          <div class=\"truck-list\">\n            <div\n              v-for=\"truck in dock.trucks\"\n              :key=\"truck.id\"\n              class=\"truck-icon\"\n              :class=\"truck.status\"\n            >\n              {{ truck.id }}\n            </div>\n          </div>\n        </div>\n        <div class=\"parking\">\n          <h3>Parking Area</h3>\n          <div class=\"truck-list\">\n            <div\n              v-for=\"parked in parkingStatus\"\n              :key=\"parked.id\"\n              class=\"truck-icon\"\n              :class=\"parked.status\"\n            >\n              {{ parked.id }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Sidebar from \"/Users/yeong-geon-macbook/Desktop/yard_management_dashboard/src/components/Cards/StatsCard.vue\";\nimport StatsCard from \"@/components/StatsCard.vue\";\n\nexport default {\n  components: {\n    Sidebar,\n    StatsCard,\n  },\n  data() {\n    return {\n      statsCards: [\n        {\n          type: \"warning\",\n          icon: \"ti-server\",\n          title: \"Total Trucks\",\n          value: \"50\",\n        },\n        {\n          type: \"success\",\n          icon: \"ti-wallet\",\n          title: \"Total Chassis\",\n          value: \"20\",\n        },\n        {\n          type: \"danger\",\n          icon: \"ti-pulse\",\n          title: \"Total Containers\",\n          value: \"10\",\n        },\n        {\n          type: \"info\",\n          icon: \"ti-twitter-alt\",\n          title: \"Total Trailers\",\n          value: \"20\",\n        },\n      ],\n      dockStatus: [\n        { name: \"Dock 01\", trucks: [{ id: \"Truck 001\", status: \"in-dock\" }, { id: \"Truck 002\", status: \"in-dock\" }] },\n        { name: \"Dock 02\", trucks: [{ id: \"Truck 003\", status: \"in-dock\" }, { id: \"Truck 004\", status: \"in-dock\" }] },\n        { name: \"Dock 03\", trucks: [{ id: \"Truck 005\", status: \"in-dock\" }, { id: \"Truck 006\", status: \"in-dock\" }] },\n      ],\n      parkingStatus: [\n        { id: \"Truck 007\", status: \"in-parking\" },\n        { id: \"Truck 008\", status: \"in-parking\" },\n        { id: \"Truck 009\", status: \"in-parking\" },\n      ],\n    };\n  },\n};\n</script>\n\n<style scoped>\n.app-wrapper {\n  display: flex;\n}\n\n.main-content {\n  flex: 1;\n  padding: 20px;\n}\n\n.stats-row {\n  display: flex;\n  gap: 20px;\n  margin-bottom: 30px;\n}\n\n.stats-card {\n  display: flex;\n  align-items: center;\n  padding: 20px;\n  border-radius: 10px;\n  background-color: #fff;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n}\n\n.icon-container {\n  font-size: 2rem;\n  margin-right: 15px;\n}\n\n.icon-warning { color: #ffc107; }\n.icon-success { color: #28a745; }\n.icon-danger { color: #dc3545; }\n.icon-info { color: #17a2b8; }\n\n.stats-info p {\n  margin: 0;\n  font-size: 0.9rem;\n  color: #888;\n}\n\n.stats-info h3 {\n  margin: 0;\n  font-size: 1.5rem;\n  font-weight: bold;\n}\n\n.yard-layout {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.dock, .parking {\n  flex: 1 1 300px;\n  padding: 15px;\n  border-radius: 8px;\n  background-color: #f8f9fa;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);\n}\n\n.dock h3, .parking h3 {\n  font-size: 1.2rem;\n  margin-bottom: 10px;\n  color: #333;\n}\n\n.truck-list {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 10px;\n}\n\n.truck-icon {\n  width: 60px;\n  height: 60px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 6px;\n  font-size: 0.9rem;\n  font-weight: bold;\n  color: #fff;\n  text-align: center;\n}\n\n.in-dock {\n  background-color: #007bff; /* Blue */\n}\n\n.in-parking {\n  background-color: #28a745; /* Green */\n}\n\n.wrong-location {\n  background-color: #dc3545; /* Red */\n}\n</style>\n"],"mappings":"AA6DA,OAAAA,OAAA;AACA,OAAAC,SAAA;AAEA;EACAC,UAAA;IACAF,OAAA,EAAAA,OAAA;IACAC,SAAA,EAAAA;EACA;EACAE,IAAA,WAAAA,KAAA;IACA;MACAC,UAAA,GACA;QACAC,IAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAH,IAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAH,IAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAH,IAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;MACA,EACA;MACAC,UAAA,GACA;QAAAC,IAAA;QAAAC,MAAA;UAAAC,EAAA;UAAAC,MAAA;QAAA;UAAAD,EAAA;UAAAC,MAAA;QAAA;MAAA,GACA;QAAAH,IAAA;QAAAC,MAAA;UAAAC,EAAA;UAAAC,MAAA;QAAA;UAAAD,EAAA;UAAAC,MAAA;QAAA;MAAA,GACA;QAAAH,IAAA;QAAAC,MAAA;UAAAC,EAAA;UAAAC,MAAA;QAAA;UAAAD,EAAA;UAAAC,MAAA;QAAA;MAAA,EACA;MACAC,aAAA,GACA;QAAAF,EAAA;QAAAC,MAAA;MAAA,GACA;QAAAD,EAAA;QAAAC,MAAA;MAAA,GACA;QAAAD,EAAA;QAAAC,MAAA;MAAA;IAEA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}